      "source": [
        "def is_leap(year):\n",
        "    leap = False\n",
        "\n",
        "    if (year>=1900 and year<=1000000):\n",
        "        if(year%4==0 and year % 100 != 0) or (year%400==0 ):\n",
        "            leap= True\n",
        "        else:\n",
        "            leap= False\n",
        "    return leap\n",
        "\n",
        "year = int(input())\n",
        "print(is_leap(year))"
